<w:WindowBase
    x:TypeArguments="vm:MainWindowViewModel"
    x:Class="ProjectName.App.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:w="clr-namespace:ProjectName.App.Windows"
    xmlns:vm="clr-namespace:ProjectName.App.Windows.ViewModels"
    d:DataContext="{d:DesignInstance vm:MainWindowViewModel}"
    mc:Ignorable="d"
    Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="{Binding Path=HelloWorldText}"/>
    </Grid>
</w:WindowBase>
